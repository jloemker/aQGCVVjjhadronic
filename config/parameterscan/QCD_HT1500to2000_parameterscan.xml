<!DOCTYPE JobConfiguration PUBLIC "" "JobConfig.dtd"[

	  <!--                   Background                   -->

	  <!ENTITY MC_QCD_HT300to500 SYSTEM "/nfs/dust/cms/user/albrechs/CMSSW_8_0_24_patch1/src/UHH2/common/datasets/RunII_80X_v3/MC_QCD_HT300to500.xml">
	  <!ENTITY MC_QCD_HT500to700 SYSTEM "/nfs/dust/cms/user/albrechs/CMSSW_8_0_24_patch1/src/UHH2/common/datasets/RunII_80X_v3/MC_QCD_HT500to700.xml">
	  <!ENTITY MC_QCD_HT700to1000 SYSTEM "/nfs/dust/cms/user/albrechs/CMSSW_8_0_24_patch1/src/UHH2/common/datasets/RunII_80X_v3/MC_QCD_HT700to1000.xml">
	  <!ENTITY MC_QCD_HT1000to1500 SYSTEM "/nfs/dust/cms/user/albrechs/CMSSW_8_0_24_patch1/src/UHH2/common/datasets/RunII_80X_v3/MC_QCD_HT1000to1500.xml">
	  <!ENTITY MC_QCD_HT1500to2000 SYSTEM "/nfs/dust/cms/user/albrechs/CMSSW_8_0_24_patch1/src/UHH2/common/datasets/RunII_80X_v3/MC_QCD_HT1500to2000.xml">
	  <!ENTITY MC_QCD_HT2000toInf SYSTEM "/nfs/dust/cms/user/albrechs/CMSSW_8_0_24_patch1/src/UHH2/common/datasets/RunII_80X_v3/MC_QCD_HT2000toInf.xml">
